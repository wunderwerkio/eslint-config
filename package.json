{
  "name": "@wunderwerk/eslint-config",
  "version": "1.0.6",
  "description": "Shared eslint config for wunderwerk projects.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --check --loglevel warn *.js",
    "format:fix": "prettier --write --loglevel warn *.js"
  },
  "keywords": [
    "eslint"
  ],
  "author": "Christian Foidl <christian@wunderwerk.io> (https://wunderwerk.io)",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">= 8",
    "eslint-plugin-import": ">= 2",
    "eslint-plugin-jsdoc": ">= 39"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 5",
    "eslint-plugin-react": ">= 7",
    "eslint-plugin-react-hooks": ">= 4"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.5.0"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "README.md",
    "LICENSE",
    "base.js",
    "index.js",
    "react.js",
    "typescript.js"
  ]
}
